# Ansible Variables Example
# Copy this file to group_vars/all.yml and customize the values

# EKS Cluster Configuration
cluster_name: "k8s-demo-dev"
aws_region: "us-west-2"

# Application Configuration
app_namespace: "demo-app"
app_version: "1.0.0"
app_replicas: 3
docker_image: "demo-k8s-app:latest"

# Environment
environment: "development"

# Features
enable_ingress: true

# Optional: Override application name
# app_name: "my-custom-app-name"